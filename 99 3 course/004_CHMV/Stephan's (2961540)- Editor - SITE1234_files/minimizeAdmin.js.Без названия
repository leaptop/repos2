function defaultParsingFlags() {
return {
empty           : false,
unusedTokens    : [],
unusedInput     : [],
overflow        : -2,
charsLeftOver   : 0,
nullInput       : false,
invalidMonth    : null,
invalidFormat   : false,
userInvalidated : false,
iso             : false
};
}
function getParsingFlags(m) {
